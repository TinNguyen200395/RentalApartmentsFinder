<?xml version='1.0' encoding='utf-8'?>
<widget id="vn.edu.greenwich.tcs19018" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RentalApartmentFinder</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="tinnvtcs19018@fpt.edu.vn" href="https://greenwich.edu.vn/">
        Tin Nguyen
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
